{
  "name": "ci-monitor",
  "displayName": "CI Monitor",
  "description": "CI Monitor for VS Code",
  "version": "0.0.2",
  "publisher": "AndriiVanikhin",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "CI Monitor",
      "properties": {
        "cimonitor.providers": {
          "type": "array",
          "title": "providers",
          "items": {
            "type": "object",
            "title": "provider config",
            "properties": {
              "id": {
                "type": "number",
                "description": ""
              },
              "name": {
                "type": "string",
                "description": ""
              },
              "login": {
                "type": "string",
                "description": ""
              },
              "password": {
                "type": "string",
                "description": ""
              },
              "serviceUrl": {
                "type": "string",
                "description": ""
              },
              "type": {
                "type": "number",
                "description": ""
              }
            }
          }
        },
        "cimonitor.jobs": {
          "type": "array",
          "items": {
            "type": "object",
            "title": "provider config",
            "properties": {
              "id": {
                "type": "number",
                "description": ""
              },
              "key": {
                "type": "string",
                "description": ""
              },
              "name": {
                "type": "string",
                "description": ""
              },
              "providerId": {
                "type": "number",
                "description": ""
              }
            }
          }
        }
      }
    }
  },
  "repository": {
    "url": "https://github.com/CRACKISH/cimonitor_vscode.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@types/node-fetch": "^2.6.2",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
